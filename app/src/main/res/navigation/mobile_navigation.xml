    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_reward"
        android:name="com.example.grafikastudio.ui.reward.RewardFragment"
        android:label="Reward"
        tools:layout="@layout/fragment_reward" />
    <fragment
        android:id="@+id/nav_profile"
        android:name="com.example.grafikastudio.ui.profile.ProfileFragment"
